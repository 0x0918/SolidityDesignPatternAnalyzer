{
    "name": "Auto Deprecation",
    "checks": [
        {
            "check_type": "modifier",
            "modifiers": ["willDeprecate", "whenDeprecated"]
        },
        {
            "check_type": "comparison",
            "binary_operations": [
                {
                    "operator": ">",
                    "operand_1": "timestamp",
                    "operand_2": "expire"
                }
            ]
        }
    ]
}